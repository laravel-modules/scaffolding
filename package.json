        "laravel-file-uploader": "^1.0.17",
        "laravel-mix": "^5.0.4",
        "laravel-mix-merge-manifest": "^0.1.2",
        "lodash": "^4.17.19",
        "pace": "0.0.4",
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.26.5",
        "sass-loader": "^8.0.0",
        "select2": "^4.1.0-beta.1",
        "toastr": "^2.1.4",
        "vali-admin": "^3.0.0",
