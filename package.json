        "tempusdominus-bootstrap-4": "^5.1.2",
        "bootstrap-switch": "3.3.4",
        "bootstrap4-duallistbox": "^4.0.1",
        "daterangepicker": "^3.0.5",
        "summernote": "^0.8.16"
